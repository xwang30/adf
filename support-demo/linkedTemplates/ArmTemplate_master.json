{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "support-demo"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks'"
		},
		"Walmart_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Walmart'"
		},
		"Walmart_repo_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Walmart_repo'"
		},
		"walmart_100_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'walmart_100'"
		},
		"walmart_150_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'walmart_150'"
		},
		"walmart_20_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'walmart_20'"
		},
		"walmart_50_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'walmart_50'"
		},
		"walmart_test_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'walmart_test'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDatabricks_accessToken": {
						"value": "[parameters('AzureDatabricks_accessToken')]"
					},
					"Walmart_accessToken": {
						"value": "[parameters('Walmart_accessToken')]"
					},
					"Walmart_repo_accessToken": {
						"value": "[parameters('Walmart_repo_accessToken')]"
					},
					"walmart_100_accessToken": {
						"value": "[parameters('walmart_100_accessToken')]"
					},
					"walmart_150_accessToken": {
						"value": "[parameters('walmart_150_accessToken')]"
					},
					"walmart_20_accessToken": {
						"value": "[parameters('walmart_20_accessToken')]"
					},
					"walmart_50_accessToken": {
						"value": "[parameters('walmart_50_accessToken')]"
					},
					"walmart_test_accessToken": {
						"value": "[parameters('walmart_test_accessToken')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}